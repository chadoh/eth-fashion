{
  "name": "eth-fashion",
  "version": "0.1.0",
  "private": true,
  "description": "Fashionable ETH clothing",
  "repository": "chadoh/eth-fashion",
  "author": "Chad Ostrowski",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "html-inline": "^1.2.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "posthtml-include": "^1.3.2",
    "sass": "^1.23.3",
